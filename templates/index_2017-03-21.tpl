<script type="text/javascript">
	function getBrand()
	{
		$.ajax({
		  url: "{_SITE_}/files/getbrand.php",
		  data: "group=" + $('#motoGroup').val(),
		  cache: false,
		  success: function(html){
			$("#motoBrand").html(html);
		  }
		});	
		$.ajax({
		  url: "{_SITE_}/files/getyear.php",
		  data: "brand=0",
		  cache: false,
		  success: function(html){
			$("#motoYear").html(html);
		  }
		});	
		$.ajax({
		  url: "{_SITE_}/files/getmodel.php",
		  data: "year=0",
		  cache: false,
		  success: function(html){
			$("#motoModel").html(html);
		  }
		});			
	}
	
	function getYear()
	{
		$.ajax({
		  url: "{_SITE_}/files/getyear.php",
		  data: "brand=" + $('#motoBrand').val(),
		  cache: false,
		  success: function(html){
			$("#motoYear").html(html);
		  }
		});	
		$.ajax({
		  url: "{_SITE_}/files/getmodel.php",
		  data: "year=0",
		  cache: false,
		  success: function(html){
			$("#motoModel").html(html);
		  }
		});			
	}

	function getModel()
	{
		$.ajax({
		  url: "{_SITE_}/files/getmodel.php",
		  data: "year=" + $('#motoYear').val(),
		  cache: false,
		  success: function(html){
			$("#motoModel").html(html);
		  }
		});			
	}	

	$(function () {
		
		
		$("#motoGroup").change(function () {
			getBrand();
		}).change();
		
		$("#motoBrand").change(function () {
			getYear();
		}).change();		

		$("#motoYear").change(function () {
			getModel();
		}).change();

		$.ajax({
		  url: "{_SITE_}/files/getgroup.php",
		  cache: false,
		  success: function(html){
			$("#motoGroup").append(html);
		  }
		});	
		
		$('#myCarousel').carousel();
	});	

	
	//
	

</script>	